{
  "name": "gcode-parser",
  "version": "1.0.0",
  "description": "Parse a gcode string into objects, while simulating the machine state.",
  "keywords": [
    "g-code",
    "gcode",
    "parser",
    "cnc"
  ],
  "main": "dist/bundle/index.js",
  "types": "dist/bundle/index.d.ts",
  "type": "module",
  "files": [
    "dist/bundle/index.d.ts",
    "dist/bundle/index.js",
    "dist/bundle/index.js.map"
  ],
  "scripts": {
    "dev": "tsx watch src/test/test_gcode.ts",
    "build": "rimraf dist && tsc && rollup --config",
    "pack_": "pnpm build && npm pack --pack-destination dist",
    "prepublish": "pnpm build",
    "test": "jest"
  },
  "author": "imivi",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/imivi/gcode-parser.git"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.1",
    "rollup-plugin-dts": "^5.3.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "tsx": "^3.12.7"
  }
